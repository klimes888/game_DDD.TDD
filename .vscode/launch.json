{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Debug NestJS gRPC",
      "runtimeArgs": ["--require", "ts-node/register"],
      "args": ["src/main.ts"],
      "cwd": "${workspaceFolder}",
      "skipFiles": ["<node_internals>/**"],
      "envFile": "${workspaceFolder}/.env",
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true
    }
  ]
}
